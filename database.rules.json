{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "config": {
      ".read": "true",
      ".write": "false",
      "private": {
        ".read": "auth !== null",
        ".write": "false"
      }
    },
    "data": {
      "$uid": {
        ".read": "auth.uid === $uid",
        ".write": "auth.uid === $uid"
      }
    }
  }
}
