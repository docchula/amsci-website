{
  "extends": ["config:base"],
  "packageRules": [
    {
      "packageNames": ["typescript"],
      "allowedVersions": ">= 2.7.2 < 2.10"
    },
    {
      "packageNames": [
        "@angular/cli",
        "@angular-devkit/build-angular"
      ],
      "groupName": "Angular CLI monorepo"
    },
    {
      "packageNames": [
        "jasmine",
        "jasmine-core"
      ],
      "groupName": "Jasmine packages"
    }
  ],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "rebaseStalePrs": true
}
